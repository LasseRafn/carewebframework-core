	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<org.carewebframework.version>3.1.0-SNAPSHOT</org.carewebframework.version>
		<org.springframework.version>3.2.5.RELEASE</org.springframework.version>
		<org.springframework.security.version>3.1.4.RELEASE</org.springframework.security.version>
		<org.apache.activemq.version>5.8.0</org.apache.activemq.version>
		<org.zkoss.zk.version>6.5.4</org.zkoss.zk.version>
		<servlet.api.version>2.5</servlet.api.version>

		<!--Necessary for passing credentials. Need respective server defined in 
			settings.xml -->
		<project.scm.id>github</project.scm.id>
		<scm.scm>scm:git</scm.scm>
		<scm.root>https://github.com/carewebframework</scm.root>
		<scm.parent>carewebframework-core</scm.parent>
		<scm.base>${scm.root}/${scm.parent}</scm.base>
		<project.name>${project.artifactId}</project.name>

		<!-- Maven Plugin Properties -->
		<maven.plugin.buildnumber.version>1.2</maven.plugin.buildnumber.version>
		<tomcat.manager.url>http://localhost:8080/manager</tomcat.manager.url>
		<tomcat.manager.server.id>tomcat.manager</tomcat.manager.server.id>
	</properties>

	<scm>
		<connection>${scm.scm}:${scm.base}</connection>
		<developerConnection>${scm.scm}:${scm.base}</developerConnection>
		<url>${scm.base}</url>
		<tag>HEAD</tag>
	</scm>

	<distributionManagement>
	</distributionManagement>

	<modules>
		<module>org.carewebframework.common</module>
		<module>org.carewebframework.mvn-parent</module>
		<module>org.carewebframework.api</module>
		<module>org.carewebframework.ui</module>
		<module>org.carewebframework.help-parent</module>
		<module>org.carewebframework.shell</module>
		<module>org.carewebframework.security.spring</module>
		<module>org.carewebframework.jms-parent</module>
		<module>org.carewebframework.ext-parent</module>
		<module>org.carewebframework.lib-parent</module>
		<module>org.carewebframework.ui-parent</module>
		<module>org.carewebframework.testharness</module>
		<module>org.carewebframework.testharness.webapp</module>
