<window title="ZK Markup" closable="true" sizable="true" width="75%"
	height="75%" controller="${xmlViewerController}">
	<panel name="panel" height="100%" width="100%">
		<panelchildren>
			<tree name="tree" hflex="1" vflex="true" />
		</panelchildren>
		<toolbar align="end">
			<label name="lblNotFound" value="Text not found"
				style="color: gray" visible="false" />
			<textbox name="textbox" forward="onOK=onSearch" width="350px"
				focus="true" />
			<button label="Search" forward="onClick=onSearch" />
		</toolbar>
	</panel>
</window>
