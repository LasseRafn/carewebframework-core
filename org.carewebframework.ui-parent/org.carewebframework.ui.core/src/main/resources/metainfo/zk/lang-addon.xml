<?xml version="1.0" encoding="UTF-8"?>
<language-addon>
	<addon-name>cwf.ui</addon-name>
	<language-name>xul/html</language-name>

	<javascript-module name="cwf" version="4.0.15" />

	<javascript src="~./org/carewebframework/ui/zk/common.js"
		merge="true" />
	<javascript src="~./org/carewebframework/ui/zk/date.js"
		merge="true" />
	<javascript src="~./org/carewebframework/ui/ext/swf/swfobject.js"
		merge="true" />
	<javascript
		src="~./org/carewebframework/ui/ext/jquery/ui/jquery-ui.min.js"
		merge="true" />
	<javascript
		src="~./org/carewebframework/ui/ext/jquery/uploadify/jquery.uploadify-3.2.1.min.js"
		merge="true" />
	<javascript
		src="~./org/carewebframework/ui/ext/jquery/jquery.base64-1.0.js"
		merge="true" />
	<javascript
		src="~./org/carewebframework/ui/ext/jquery/jquery.watermark.js"
		merge="true" />
	<javascript
		src="~./org/carewebframework/ui/ext/bootstrap/js/bootstrap.min.js"
		merge="true" />

	<stylesheet
		href="~./org/carewebframework/ui/ext/jquery/ui/jquery-ui.min.css"
		type="text/css" />
	<stylesheet
		href="~./org/carewebframework/ui/ext/jquery/ui/jquery-ui.structure.min.css"
		type="text/css" />
	<stylesheet
		href="~./org/carewebframework/ui/ext/jquery/ui/jquery-ui.theme.min.css"
		type="text/css" />
	<stylesheet
		href="~./org/carewebframework/ui/ext/bootstrap/css/bootstrap.min.css"
		type="text/css" />
	<!-- common.css.dsp should be last in list -->
	<stylesheet href="~./org/carewebframework/ui/zk/common.css.dsp"
		type="text/css" />

	<component>
		<component-name>datepicker</component-name>
		<extends>combobox</extends>
		<component-class>org.carewebframework.ui.zk.DateRangePicker
		</component-class>
	</component>
	<component>
		<component-name>datechooser</component-name>
		<extends>listbox</extends>
		<component-class>org.carewebframework.ui.zk.DateRangeChooser
		</component-class>
	</component>
	<component>
		<component-name>datebox</component-name>
		<extends>datebox</extends>
		<component-class>org.carewebframework.ui.zk.DateboxEx
		</component-class>
		<widget-class>cwf.ext.Datebox
		</widget-class>
		<property>
			<property-name>lenient</property-name>
			<property-value>true</property-value>
		</property>
	</component>
	<component>
		<component-name>datetimebox</component-name>
		<extends>bandbox</extends>
		<component-class>org.carewebframework.ui.zk.DateTimebox
		</component-class>
	</component>
	<component>
		<component-name>selectiongrid</component-name>
		<extends>grid</extends>
		<component-class>org.carewebframework.ui.zk.SelectionGrid
		</component-class>
	</component>
	<component>
		<component-name>colorpicker</component-name>
		<extends>bandbox</extends>
		<component-class>org.carewebframework.ui.zk.ColorPicker
		</component-class>
		<widget-class>cwf.ext.ColorPicker
		</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<css-uri>css/ColorPicker.css.dsp</css-uri>
		</mold>
	</component>
	<component>
		<component-name>iconpicker</component-name>
		<extends>bandbox</extends>
		<component-class>org.carewebframework.ui.zk.IconPicker
		</component-class>
		<widget-class>cwf.ext.IconPicker
		</widget-class>
		<mold>
			<mold-name>default</mold-name>
		</mold>
	</component>
	<component>
		<component-name>iconpickerEx</component-name>
		<extends>bandbox</extends>
		<component-class>org.carewebframework.ui.icons.IconPickerEx
		</component-class>
		<widget-class>cwf.ext.IconPicker
		</widget-class>
		<mold>
			<mold-name>default</mold-name>
		</mold>
	</component>
	<component>
		<component-name>splitterview</component-name>
		<component-class>org.carewebframework.ui.zk.SplitterView
		</component-class>
		<widget-class>cwf.ext.SplitterView
		</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>mold/SplitterView.js</mold-uri>
			<css-uri>css/SplitterView.css.dsp</css-uri>
		</mold>
	</component>
	<component>
		<component-name>splitterpane</component-name>
		<component-class>org.carewebframework.ui.zk.SplitterPane
		</component-class>
		<widget-class>cwf.ext.SplitterPane
		</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>mold/SplitterPane.js</mold-uri>
		</mold>
	</component>
	<component>
		<component-name>capslocklabel</component-name>
		<extends>label</extends>
		<component-class>org.carewebframework.ui.zk.CapsLockLabel
		</component-class>
		<widget-class>cwf.ext.CapsLockLabel</widget-class>
	</component>
	<component>
		<component-name>labeledelement</component-name>
		<component-class>org.carewebframework.ui.zk.LabeledElement
		</component-class>
		<widget-class>cwf.ext.LabeledElement</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>mold/LabeledElement.js</mold-uri>
			<css-uri>css/LabeledElement.css.dsp</css-uri>
		</mold>
	</component>
	<component>
		<component-name>messagewindow</component-name>
		<component-class>org.carewebframework.ui.zk.MessageWindow
		</component-class>
		<widget-class>cwf.ext.MessageWindow</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>mold/MessageWindow.js</mold-uri>
			<css-uri>css/MessageWindow.css.dsp</css-uri>
		</mold>
	</component>
	<component>
		<component-name>menuEx</component-name>
		<component-class>org.carewebframework.ui.zk.MenuEx
		</component-class>
		<extends>menu</extends>
		<widget-class>cwf.ext.MenuEx</widget-class>
	</component>
	<component>
		<component-name>window</component-name>
		<extends>window</extends>
		<widget-class>cwf.ext.Window</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>/js/zul/wnd/mold/window.js</mold-uri>
		</mold>
		<mold>
			<mold-name>bootstrap</mold-name>
			<mold-uri>/js/cwf/ext/mold/Window.js</mold-uri>
		</mold>
	</component>
	<component>
		<component-name>panel</component-name>
		<extends>panel</extends>
		<widget-class>cwf.ext.Panel</widget-class>
		<mold>
			<mold-name>default</mold-name>
			<mold-uri>/js/zul/wnd/mold/panel.js</mold-uri>
		</mold>
		<mold>
			<mold-name>bootstrap</mold-name>
			<mold-uri>/js/cwf/ext/mold/Panel.js</mold-uri>
		</mold>
	</component>
</language-addon>
