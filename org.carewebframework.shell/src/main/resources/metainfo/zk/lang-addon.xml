<language-addon>
	<addon-name>cwf.shell</addon-name>
	<depends>cwf.ui</depends>
	<language-name>xul/html</language-name>

	<javascript-module name="cwf" version="3.1.0"/>

	<javascript>
		cwf.registerPrintStyle('~./org/carewebframework/shell/careWebPrint.css');
	</javascript>

	<stylesheet href="~./org/carewebframework/shell/careWeb.css"
		type="text/css" />
		
	<component>
		<component-name>cwfShell</component-name>
		<extends>div</extends>
		<component-class>org.carewebframework.shell.CareWebShell
		</component-class>
	</component>
		
	<component>
		<component-name>cwfShellEx</component-name>
		<extends>cwfShell</extends>
		<component-class>org.carewebframework.shell.CareWebShellEx
		</component-class>
	</component>
		
</language-addon>